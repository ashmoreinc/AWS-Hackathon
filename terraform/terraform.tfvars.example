# terraform.tfvars.example
# Copy this file to terraform.tfvars and customize for your deployment

# AWS Region
aws_region = "us-east-1"

# Project name (used for resource naming)
project_name = "hackathon"

# Environment (dev, staging, prod)
environment = "dev"

# Optional: Custom domain name for CloudFront
# domain_name = "yourdomain.com"

# IAM role ARNs that can deploy to the static website S3 bucket
# Add your SSO role ARN here to allow deployment
# deployer_role_arns = ["arn:aws:iam::123456789012:role/AWSReservedSSO_DeveloperAdministratorAccess_xxxx"]
